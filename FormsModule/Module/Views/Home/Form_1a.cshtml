                </tr>
                <tr>
                    <td>Citizenship of Father</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.citizenship_of_father, new { @class = "form-control", @placeholder = "Citizenship of Father" })</td>
                </tr>
                <tr>
                    <td>Date of Marriage of Parents</td>
                    <td>:</td>
                    <td>@Html.EditorFor(model => model.date_of_marriage_of_parents, new { @class = "datepicker", @placeholder = "Date of Marriage of Parents" })</td>
                </tr>
                <tr>
                    <td>Place of Marriage of Parents</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.place_of_marriage_of_parents, new { @class = "form-control", @placeholder = "Place of Marriage of Parents" })</td>
                </tr>
                <tr>
                    <td>Issued to</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.issued_to, new { @class = "form-control", @placeholder = "Issued To" })</td>
                </tr>
                <tr>
                    <td>Officer</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.officer_name, new { @class = "form-control", @placeholder = "Officer Name" })</td>
                </tr>
                <tr>
                    <td>Officer Title</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.officer_title, new { @class = "form-control", @placeholder = "Officer Title" })</td>
                </tr>
                <tr>
                    <td>Verifier</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.verifier_name, new { @class = "form-control", @placeholder = "Verifier Name" })</td>
                </tr>
                <tr>
                    <td>Verifier Title</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.verifier_title, new { @class = "form-control", @placeholder = "Verifier Title" })</td>
                </tr>
                <tr>
                    <td>Amount Paid</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.payment, new { @class = "form-control", @placeholder = "Payment" })</td>
                </tr>
                <tr>
                    <td>O.R. Number</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.or_no, new { @class = "form-control", @placeholder = "OR Number" })</td>
                </tr>
                <tr>
                    <td>Date Paid</td>
                    <td>:</td>
                    <td>@Html.EditorFor(model => model.date_paid, new { @class = "datepicker", @placeholder = "Date Paid" })</td>
                </tr>
            </table><br>
            <input type="Submit" value="Submit" class="btn btn-block btn-primary" id="btnSubmit" />
        </form>

    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/jquery-ui.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/js/bootstrap-datepicker.min.js"></script>
   
    <script type="text/javascript">
        var date = new Date();
        var today = new Date(date.getFullYear(), date.getMonth(), date.getDate());

        var optSimple = {
            format: 'mm-dd-yyyy',
            todayHighlight: true,
            orientation: 'bottom right',
            autoclose: true,
            inline: true,
        };

        $('#entry').datepicker(optSimple);
        $('#entry').datepicker('setDate', today);

        $(document).ready(function () {
            $("#btnSubmit").click(function () {
                debugger
                var data = $("#myForm").serialize();
                $.ajax({
                    type: "POST",
                    url: "/Home/form_1a",
                    data: data,
                    success: function (response) {
                        alert("Successful");
                    }
                })
            })
        })
    </script>
</body>
</html>
