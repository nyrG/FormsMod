@{
    ViewBag.Title = "form_1a";
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />   
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />

</head>
<body>
    <div class="container" style="width:40%;margin-top:2%">
        <form id="myForm">
            Fill-up All Fields
            <table>
               
                <tr>
                    <td>Date</td>
                    <td>:</td>
                    
                    <td>@Html.TextBoxFor(model => model.entry, new { @class = "form-control" , @id="entry"})</td>
                </tr>
                <tr>
                    <td>Page</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.page, new { @class = "form-control", @placeholder = "Page Number" })</td>
                </tr>
                <tr>
                    <td>Book Number</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.book, new { @class = "form-control", @placeholder = "Book Number" })</td>
                </tr>
                <tr>
                    <td>LCR Registry Number</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.lcr_reg_no, new { @class = "form-control", @placeholder = "LCR Registration Number" })</td>
                </tr>
                <tr>
                    <td>Date of Registration</td>
                    <td>:</td>
                    <td>@Html.EditorFor(model => model.date_of_reg, new { @class = "datepicker", @placeholder = "Date of Registration" })</td>
                </tr>
                <tr>
                    <td>Name of Child</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.name_child, new { @class = "form-control", @placeholder = "Name of Child" })</td>
                </tr>
                <tr>
                    <td>Sex</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.sex, new { @class = "form-control", @placeholder = "Sex" })</td>
                </tr>
                <tr>
                    <td>Date of Birth</td>
                    <td>:</td>
                    <td>@Html.EditorFor(model => model.date_of_birth, new { @class = "datepicker", @placeholder = "Date of Birth" })</td>
                </tr>
                <tr>
                    <td>Place of Birth</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.place_of_birth, new { @class = "form-control", @placeholder = "Place of Birth" })</td>
                </tr>
                <tr>
                    <td>Name of Mother</td>
                    <td>:</td>
                    <td>@Html.TextBoxFor(model => model.name_of_mother, new { @class = "form-control", @placeholder = "Name of Mother" })</td>
                </tr>
                <tr>
                    <td>Citizenship of Mother</td>
                    <td>:</td>
